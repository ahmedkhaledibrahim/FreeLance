
<html>
<head>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"
            type="text/javascript">
    </script>
    @Styles.Render("~/Content/css")

    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"
            type="text/javascript">
    </script>

   
    <title>Profile</title>
    <link rel="stylesheet" href="~/Scripts/StartPage.css" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Start</title>
    <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.min.js" integrity="sha384-Atwg2Pkwv9vp0ygtn1JAojH0nYbwNJLPhwyoVbhoPwBhjQPR5VtM2+xf0Uwh9KtT" crossorigin="anonymous"></script>
</head>
<body>
    <div style="  position: fixed; top: 0; width: 100%; z-index: 100;height: 50px; background-color: #1d232b; " class="navbar navbar-inverse navbar-fixed-top text-white">

        <div style="margin-top:8px; margin-left:57px;">
            <label style="color:white;margin-right: 1100px; font-size:medium">
                F-Lancing
            </label>
        </div>
        <div class="navbar-collapse collapse" style="position:fixed; left:1000px; margin-left:300px;">
            <ul class="nav navbar-nav">
                <li>

                    @Html.ActionLink("Logout", "ModalPopUp", "Start", null, new { @class = "btn text-danger my-2 my-sm-0", @style = "font-size:medium;" })

                </li>
            </ul>
        </div>

    </div>





    <div class="d-flex flex-column flex-shrink-0 p-3 text-white" style="background-color: #1d232b;  height: 100%;width: 250px; float: left;">
       
        <hr>
        <ul class="nav nav-pills flex-column mb-auto">
            <li class="nav-item">
                <a href="~/Start/ReturnToAfterLogin" class="nav-link active" aria-current="page">
                    <svg class="bi me-2" width="16" height="16"><use xlink:href="#home"></use></svg>
                    Profile
                </a>
            </li>
            <li>

                <a href="~/Clientposts/CreateNewPost" class="nav-link text-white">
                    <svg class="bi me-2" width="16" height="16"><use xlink:href="#speedometer2"></use></svg>
                    Create New Post
                </a>

            </li>
            <li>
                <a href="~/Clientposts/MyPosts" class="nav-link text-white">
                    <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"></use></svg>
                    My Posts
                </a>
            </li>
           

        </ul>


    </div>
    <div style="margin-left: 400px;margin-top:30px; width:600px;height:200px;">

        @RenderBody()
    </div>
    <footer class=" fixed-bottom font-small bg-dark">
        <div class="footer-copyright text-center text-light">
            © 2021 Copyright:

        </div>
    </footer>
</body>
</html >